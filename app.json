{
  "expo": {
    "name": "Raigamsfa",
    "slug": "raigamsfa",
    "version": "1.0.0",
     "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "raigamsfa",
    
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#FFFFFF"
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@mui/icons-material",
          "expo-checkbox",
          "undici"
        ]
      }
    },
    "cli": {
      "appVersionSource": "remote"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raigam.Raigamsfa",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to enhance your experience.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location in the background for tracking purposes.",
        "NSLocationAlwaysUsageDescription": "This app uses your location in the background for tracking purposes."
      },
      "UIBackgroundModes": [
        "location",
        "fetch"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFFFF",
        "versionCode": 2
      },
      "edgeToEdgeEnabled": false,
      "package": "com.raigams.Raigamsfa",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0C056D",
          "image": "./assets/images/splash_screen_logo.png",
          "resizeMode": "contain",
          "imageResizeMode": "contain"
        
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "92c42144-c096-47a3-8ff4-1150faf65f9a"
      }
    },
    "owner": "raigamsfa"
  }
}
