{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,

    "jsx": "react", // or "react-native" for React Native projects
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]




  
}
